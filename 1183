#include<stdio.h>
int main()
{
    double array[12][12],sum=0;
    char c[2];
    int i,j;
    scanf("%s",&c);
    for(i=0;i<12;i++)
    {
        for(j=0;j<12;j++)
        {
            scanf("%lf",&array[i][j]);
            if(i<j)
                sum+=array[i][j];
        }
    }
    if(c[0]=='S')
        printf("%.1lf\n",sum);
    else if(c[0]=='M')
        printf("%.1lf\n",sum/66.0);
    return 0;

}


