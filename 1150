#include<stdio.h>
int main()
{
    int a,b,i,c=0,sum=0;
    scanf("%d%d",&a,&b);
    while(1)
    {
        scanf("%d",&b);
        if(b>a)
            break;
    }
    i=a;
    for(;;)
    {
        sum=sum+i;
        i++;
        c++;
        if(sum>b)
            break;
    }
    printf("%d\n",c);
}
