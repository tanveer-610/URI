#include<stdio.h>
int main()
{
    int i,n,m[10000];
    scanf("%d",&n);

    for(i=0;i<n;i++)
    {
        scanf("%d",&m[i]);

    }
    for(i=0;i<n;i++)
    {
         if(m[i]%2!=0 && m[i]>0)
            printf("ODD POSITIVE\n");
        else if(m[i]%2!=0 && m[i]<0)
            printf("ODD NEGATIVE\n");
        else if(m[i]==0)
                printf("NULL\n");
        else if(m[i]%2==0 && m[i]>0)
            printf("EVEN POSITIVE\n");
        else if(m[i]%2==0 && m[i]<0)
            printf("EVEN NEGATIVE\n");
    }

}
