#include<stdio.h>
int main()
{
    int i,count=0;
    double n[6],sum=0;
    for(i=0;i<6;i++)
    {
        scanf("%lf",&n[i]);
        if(n[i]>0)
        {
            sum=sum+n[i];
            count=count+1;
        }
   }
    printf("%d valores positivos\n",count);
    printf("%.1f\n",sum/count);
    return 0;
}

