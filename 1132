#include<stdio.h>
int main()
{
    int n=13,a,b,sum=0,i;
    scanf("%d%d",&a,&b);

    if(a>b)
    {
        for(i=b;i<=a;i++)
        {
            if(i%n!=0)
                sum=sum+i;
        }
    }
     else if(a<b)
    {
        for(i=a;i<=b;i++)
        {
            if(i%n!=0)
                sum=sum+i;
        }
    }
    printf("%d\n",sum);
    return 0;
}
