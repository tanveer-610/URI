#include<stdio.h>
int main()
{
    int n,al=0,ga=0,di=0;
    while(1)
    {
        scanf("%d",&n);
        if(n==1)
            al++;
        else if(n==2)
            ga++;
        else if(n==3)
            di++;
        else if(n==4)
            break;
    }
    printf("MUITO OBRIGADO\nAlcool: %d\nGasolina: %d\nDiesel: %d\n",al,ga,di);
    return 0;
}
