#include<stdio.h>
int main()
{
   double a,b,c,pi = 3.14159,tri,cir,trap,sq,rec;
   scanf("%lf %lf %lf",&a,&b,&c);

   tri=.5*a*c;
   cir=pi*c*c;
   trap=(0.5*(a+b))*c;
   sq=b*b;
   rec=a*b;
   printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n",tri,cir,trap,sq,rec);
    return 0;
}
