#include<stdio.h>
int main()
{
    double array[12][12],sum=0;
    char c[2];
    int i,j,count=0;
    scanf("%s",&c);
    for(i=0;i<12;i++)
    {
        for(j=0;j<12;j++)
        {
            scanf("%lf",&array[i][j]);
            if(j<i && j<(11-i)){
                sum+=array[i][j];
                count++;
            }
        }
    }
    if(c[0]=='S')
        printf("%.1lf\n",sum);
    else if(c[0]=='M')
        printf("%.1lf\n",sum/count);
    return 0;

}
